_ do [
    _version set _ import _language.version,
    _generate set _ import _language.syntax.generate,
    _join set _ import _text.join,
    [major, minor, patch] = version,
    _major set _ generate major,
    _minor set _ generate minor,
    _patch set _ generate patch,
    "" join ["v", major, ".", minor, ".", patch]
]
