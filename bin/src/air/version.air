_ do [
    .version set _ import _language.version,
    .generate set _ import _language.syntax.generate,
    .join set _ import _text.join,
    [.major, .minor, .patch] = version,
    .major set _ generate major,
    .minor set _ generate minor,
    .patch set _ generate patch,
    "" join ["v", major, ".", minor, ".", patch]
]
