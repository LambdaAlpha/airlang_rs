new get
-----
do [
    f = any ; .,
    i = any ; .,
    a = f '!' i,
    (f == a optimize.function .) and (i == a optimize.input .)
]
-----
true
=====
set get
-----
do [
    a = any ; optimize,
    f = any ; .,
    i = any ; .,
    a optimize.set_function f,
    a optimize.set_input i,
    (f == a optimize.function .) and (i == a optimize.input .)
]
-----
true
=====
set return
-----
do [
    f1 = any ; .,
    i1 = any ; .,
    a = f1 '!' i1,
    f2 = a optimize.set_function any ; .,
    i2 = a optimize.set_input any ; .,
    (f1 == f2) and i1 == i2
]
-----
true
=====
apply
-----
do [c = id ; 1 ! 2, optimize.apply ^c]
