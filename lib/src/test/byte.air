length join
-----
_ do [
    a = _ any byte,
    la = a ; byte.length .,
    b = _ any byte,
    lb = b ; byte.length .,
    ab = . byte.join [#a, #b],
    lab = ab ; byte.length .,
    lab == la + lb
]
