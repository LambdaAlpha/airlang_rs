new
-----
do [
    f = any ; .,
    o = any ; .,
    a1 = f '?' o,
    a2 = form* ; f ? o,
    a1 == a2
]
-----
true
=====
new get
-----
do [
    f = any ; .,
    o = any ; .,
    a = f '?' o,
    (f == a solve.function .) and (o == a solve.output .)
]
-----
true
=====
set get
-----
do [
    a = any ; solve,
    f = any ; .,
    o = any ; .,
    a solve.set_function f,
    a solve.set_output o,
    (f == a solve.function .) and (o == a solve.output .)
]
-----
true
=====
set return
-----
do [
    f1 = any ; .,
    o1 = any ; .,
    a = f1 '?' o1,
    f2 = a solve.set_function any ; .,
    o2 = a solve.set_output any ; .,
    (f1 == f2) and o1 == o2
]
-----
true
