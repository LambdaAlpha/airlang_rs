new
-----
do [
    f = any ; .,
    s = any ; .,
    p1 = f ':' s,
    p2 = f : s,
    p1 == p2
]
-----
true
=====
new get
-----
do [
    f = any ; .,
    s = any ; .,
    p = f ':' s,
    (f == p pair.first .) and (s == p pair.second .)
]
-----
true
=====
set get
-----
do [
    p = any ; pair,
    f = any ; .,
    s = any ; .,
    p pair.set_first f,
    p pair.set_second s,
    (f == p pair.first .) and (s == p pair.second .)
]
-----
true
=====
set return
-----
do [
    f1 = any ; .,
    s1 = any ; .,
    p = f1 : s1,
    f2 = p pair.set_first any ; .,
    s2 = p pair.set_second any ; .,
    (f1 == f2) and s1 == s2
]
-----
true
