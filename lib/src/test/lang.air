_ do [
    .any set _ import _value.any,
    .parse set _ import _language.syntax.parse,
    .generate set _ import _language.syntax.generate,
    .get set _ import _list.get,
    .% set _ import _integer.remainder,
    .atom set [_unit, _bit, _symbol, _text, _integer, _number, _byte],
    .i set 3 + (_ any _integer) % 4,
    .a set _ any atom which _ get i,
    (_ any _bit) loop [
        (_ any _bit) test [
            .i set 3 + (_ any _integer) % 4,
            .b set _ any atom which _ get i,
            .i set (4 + (_ any _integer) % 4) % 4,
            .a set (_ any _bit) test [a : b] : [_ form _ a b],
        ] : [
            (_ any _bit) test [
                .i set 3 + (_ any _integer) % 4,
                .b set _ any atom which _ get i,
                .a set [a, b],
            ] : [
                .i set 3 + (_ any _integer) % 4,
                .b set _ any atom which _ get i,
                .a set {a : b},
            ]
        ]
    ],
    .a1 set _ parse _ generate a,
    a == a1
]
