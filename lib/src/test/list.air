set_many get_many
-----
_ do [
    a = _ any list,
    la = a ; list.length .,
    (la == 0) .return true,
    b = _ any list,
    lb = b ; list.length .,
    (lb == 0) .return true,
    % = _ import .integer.remainder,
    (la < lb) ? [
        t = a,
        a = b,
        b = t,
        t = la,
        la = lb,
        lb = t,
    ] : [],
    a1 = a,
    d = la - lb,
    i = (d == 0) ? [0] : [(d + (_ any integer) % d) % d],
    c1 = a ; list.get_many i : i + lb,
    c2 = a ; list.set_many i : b,
    b1 = a ; list.get_many i : i + lb,
    a ; list.set_many i : c1,
    (b == b1) and (c1 == c2) and (a == a1)
]
