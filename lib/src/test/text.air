length push
-----
_ do [t = _ any text, l = t | text.length ., t | text.push "a", (t | text.length .) == l + 1]
-----
true
=====
length join
-----
_ do [
    a = _ any text,
    la = a | text.length .,
    b = _ any text,
    lb = b | text.length .,
    ab = . text.join [#a, #b],
    lab = ab | text.length .,
    lab == la + lb
]
-----
true
=====
length empty
-----
_ do [t = "", t | text.length .]
-----
0
=====
length else
-----
_ do [t = "1", t | text.length .]
-----
1
