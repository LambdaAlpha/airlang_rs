new
-----
do [
    f = any ; .,
    i = any ; .,
    a1 = f '!' i,
    a2 = form ; *f ! *i,
    a1 == a2
]
-----
true
=====
new get
-----
do [
    f = any ; .,
    i = any ; .,
    a = f '!' i,
    (f == abstract.function ; a) and (i == abstract.input ; a)
]
-----
true
=====
set get
-----
do [
    a = any ; abstract,
    f = any ; .,
    i = any ; .,
    a abstract.set_function f,
    a abstract.set_input i,
    (f == abstract.function ; a) and (i == abstract.input ; a)
]
-----
true
=====
set return
-----
do [
    f1 = any ; .,
    i1 = any ; .,
    a = f1 '!' i1,
    f2 = a abstract.set_function any ; .,
    i2 = a abstract.set_input any ; .,
    (f1 == f2) and i1 == i2
]
-----
true
