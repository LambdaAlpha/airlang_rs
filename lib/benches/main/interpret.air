_("bench interpret")

_ do [
    .% set _ import _integer.remainder,
    .f set _ function {
        code : .n : _ form _ do [
            ..sum set 0,
            .n iterate .i : [
                _ _try (0 == .i % 3) test [continue],
                ..sum set .sum + .i,
            ],
            .sum
        ],
        context_free : true,
        prelude : {},
    },
    _ f 100
]
