"bench interpret" @ ; ! [
  ctx = . context {
    : ;,
    : =,
    : for,
    : ==,
    : %,
    : +,
  },
  f = function ! {
    body : ; ! [
      sum = 0,
      the_input for i : [
        (0 == i % 3) continue .,
        sum = sum + i,
      ],
      sum
    ],
    prelude : move ! ctx,
  },
  f 100
]
