"bench interpret" @ ; ! [
  ctx = context ! {
    variables : {
      : ;,
      : =,
      : for,
      : ==,
      : %,
      : +,
    }
  },
  f = function ! {
    input_name : .n,
    body : ; ! [
      sum = 0,
      n for i : [
        (0 == i % 3) continue .,
        sum = sum + i,
      ],
      sum
    ],
    prelude : &ctx,
  },
  f 100
]
