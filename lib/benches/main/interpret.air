"bench interpret" @ ; $ [
  ctx = 'u context {
    : ;,
    : while,
    : <>,
    : if,
    : >,
    : =,
    : -,
  },
  f = function $ {
    body : ; $ [
      (x : y) = input,
      while [
        x <> y,
        if [
          x > y,
          x = x - y,
          y = y - x
        ]
      ],
      x
    ],
    context : move $ ctx,
  },
  x = 42,
  y = 24,
  x f y
]
