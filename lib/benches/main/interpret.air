_("bench interpret")

_ do [
    % = _ import _integer.remainder,
    f = _ function {
        code : _n : _ do [
            _sum = 0,
            _n iterate _i : [
                (0 == _i % 3) __continue .,
                _sum = _sum + _i,
            ],
            _sum
        ],
        context_access : _free,
    },
    _ f 100
]
