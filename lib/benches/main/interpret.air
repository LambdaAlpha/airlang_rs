_("bench interpret")

_ do [
    .% set _ import _integer.remainder,
    .f set _ function {
        code : .n : _ form _ do [
            ..sum set 0,
            .n iterate .i : [
                (0 == .i % 3) _continue .,
                ..sum set .sum + .i,
            ],
            .sum
        ],
        context_access : _free,
        context : {},
    },
    _ f 100
]
